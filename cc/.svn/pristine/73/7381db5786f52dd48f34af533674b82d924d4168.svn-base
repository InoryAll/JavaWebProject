/* login.css style*/
*{
	margin: 0;
	padding: 0;
	font-family: '微软雅黑';
}
li{
	list-style: none;
}
a{
	text-decoration: none;
}
table{
	border-collapse: collapse;
}
img{
	border: none;
}
html{
	width: 100%;
	height:100%;
}
#header_box{
	width: 100%;
	height:100%;
	background:#438eb9;
	min-width: 1348px;
}
#header{
	width:1348px;
	height: 46px;
	margin: 0 auto;
}
.logo{
	display: block;
	width: 300px;
	height: 46px;
	text-align: center;
	font-size: 20px;
	line-height: 46px;
	color: #fff;
}
#main_box{
	width: 100%;
	height:100%;
	background: #f2f2f2;
	min-width: 1348px;
	min-height: 600px;
}
#main{
	width: 1348px;
	height: 600px;
	min-height: 600px;
	margin: 0 auto;
	position: relative;
}
#main_left{
	position: relative;
	width: 674px;
	height: 600px;
	float: left;
}
#main_right{
	position: relative;
	width: 674px;
	height: 600px;
	float: left;
}
.picTab_box{
	position: absolute;
	width: 600px;
	height: 400px;
	left:50px;
	top: 100px;
	overflow: hidden;
}
#picTab{
	position: absolute;
	left: 0px;
	width:1800px;
	height: 400px;
}
#picTab li{
	width: 600px;
	height: 400px;
	float: left;
}
#scrollTab{
	position: absolute;
	width: 100px;
	height: 20px;
	bottom:64px;
	left: 296px;	
}
#scrollTab li{
	float: left;
	width: 16px;
	height: 16px;
	border-radius: 10px;
	margin-left: 10px;
	background: #94a59c;
	border: 2px solid #fff;
}
#picTab img{
	width: 600px;
	height: 400px;
}
#login_table{
	position: absolute;
	width: 378px;
	height: 262px;
	left:130px;
	top:150px;
}
#login_title{
	height: 50px;
}
#login_table h1{
	display: block;
	font-size:20px;
	color: #47a9dd;
	margin-left: 30px;
}
.split{
	border: 1px solid #d5e3ef;
	margin-left: 30px;
	margin-bottom: 20px;
}
#username_div,#password_div{
	width: 290px;
	height: 30px;
	outline: none;
	border: 1px solid #d5d5d5;
	margin-left: 30px;
	background: #fff;
}
#username_div input,#password_div input{
	height: 22px;
	margin:4px 6px;
	width: 260px;
	border: none;
	font-size: 18px;
	float: left;
	background: none;
	outline: none;
}
#username_div span,#password_div span{
	display: block;
	width: 16px;
	height: 22px;
	margin-top: 4px;
	float: left;
}
#password_div span{
	margin-left: 2px;
}
#login_btn{
	display: block;
	width:100px;
	height: 36px;
	line-height: 36px;
	text-align: center;
	border: none;
	background: #428bca;
	color:#fff;
	font-weight: bold;
	margin-top: 24px;
	margin-left: 220px;
}

#regedit{
	display: block;
	float: left;
	text-decoration: underline; 
	color: #47a9dd;
	margin-top: 36px;
	margin-left: 56px;
}
#submit_btn:hover{
	background: #87b87f;
}
#regedit:hover{
	color: #438eb9;
}
#footer_box{
	width: 100%;
	height:100%;
	background: #f2f2f2;
	min-width: 1348px;
}
#footer{
	width: 1348px;
	height: 100px;
	text-align: center;
	margin: 0 auto;	
}
.split_bottom{
	width: 1288px;
	border: 1px solid #d5e3ef;
	margin-left: 30px;
}
#copy{
	display: block;
	width: 1000px;
	height: 30px;
	margin:8px auto ;
}
#character{	
	display: none;
	position: absolute;
	width:400px; 
	height: 220px;
	left: 0px;
	top:0px ;
	background: #f7f7f7;
	border:10px solid #fff;
	opacity: 1;
}
#character h1{
	display: block;
	width: 400px;
	height: 100px;
	color: #478fca;
	text-align: center;
	line-height: 100px;
}
#character span{
	display: block;
	width:20px;
	height: 20px;
	position: absolute;
	right:2px; 
	top:2px;
	text-align: center;
	line-height:20px;
}
#character span img{
	width: 10px;
	height: 10px;
}
#character input{
	border: none;
	display: block;
	width: 150px;
	height: 50px;
	float: left;
	line-height: 50px;
	color: #fff;
	text-align: center;
	font-weight: bold;
	font-size: 20px;
	margin-top: 20px;
}
#student_user{
	margin-left: 40px;
	background:#d36e6e;
}
#teacher_user{
	margin-left: 10px;
	background:#efad62;
}
#message,#ReturnMessage{
	display: block;
	width: 290px;
	height: 30px;
	margin-left: 31px;
	margin-top: 6px;
	font-size: 18px;
	color: red;
	font-weight: bold;
}
#login_form{
	width: 100%;
	height: 100%;
}
#teacher_regedit{
	display: block;
	float: left;
	text-decoration: underline;
	color: #47a9dd;
	margin-top: 36px;
	margin-left: 6px;
}
