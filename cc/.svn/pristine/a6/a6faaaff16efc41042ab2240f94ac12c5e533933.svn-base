/*regedit.css style*/
*{
	margin: 0;
	padding: 0;
}
li{
	list-style: none;
}
table{
	border-collapse: collapse;
}
a{
	text-decoration: none;
}
img{
	border: none;
}
html{
	width:100%;
	height:100%;
	background:#f5f5f5;
}
#header_box{
	width: 100%;
	height:100%;
	background:#438eb9;
	min-width: 1348px;
}
#header{
	width:1348px;
	height: 46px;
	margin: 0 auto;
}
.logo{
	display: block;
	width: 300px;
	height: 46px;
	text-align: center;
	font-size: 20px;
	line-height: 46px;
	color: #fff;
}
#main_box{
	width: 100%;
	height: 100%;
	min-height: 680px;
}
#main{
	width:1348px;
	height: 680px;
	min-height: 680px;
	margin: 0 auto;
	position: relative;
}
#table_box{
	position: absolute;
	width: 1348px;
	height: 680px;
	margin: 0 auto;
	top: 40px;
}
#regedit_table1{
	width: 800px;
	height: 450px;
	background: #fff;
	margin: 0px auto;
	border: 1px solid #dddddd;
	border-top: none;
}
#regedit_table1 tr{
	display: block;
	width: 800px;
	height: 60px;
	line-height: 60px;
}
#regedit_table1 td{
	display: block;
	width: 800px;
	height: 34px;
	padding-top: 14px;

}
#regedit_table2{
	width: 800px;
	height: 450px;
	background: #fff;
	margin: 0px auto;
	border: 1px solid #dddddd;
	border-top: none;
	display: none;
}
#regedit_table2 tr{
	display: block;
	width: 800px;
	height: 60px;
	line-height: 60px;
}
#regedit_table2 td{
	display: block;
	width: 800px;
	height: 34px;
	padding-top: 14px;

}
#regedit_table3{
	width: 800px;
	height: 450px;
	background: #fff;
	margin: 0px auto;
	border: 1px solid #dddddd;
	border-top: none;
	display: none;
}
#regedit_table3 tr{
	display: block;
	width: 800px;
	height: 80px;
	line-height: 80px;
}
#regedit_table3 td{
	display: block;
	width: 800px;
	height: 60px;
	padding-top: 14px;

}
.title{
	width: 800px;
	height:40px;
	background: #f7f7f7;
	border: 1px solid #dddddd;
	margin-left: 273px;
}
.title p{
	width: 800px;
	height:40px;
	margin-left: 10px;	
	text-align: left;
	font-weight: bold;
	color:#7cc2e6;
	font-size: 16px;
	line-height: 40px;
}
#nav{
	width: 800px;
	height: 100px;
	border: 1px solid #dddddd;
	background: #fff;
	margin-left: 273px;
	border-bottom: none;
}
#nav li{
	position: relative;
	float: left;
	width: 200px;
	height: 100px;
	margin-left: 50px;
}
.step_circle{
	position: absolute;
	width: 50px;
	height:50px; 
	border:6px solid #ced1d6;
	border-radius:30px;
	text-align: center;
	line-height: 50px;
	color: #546474;
	font-size: 20px;
	font-weight: bold;
	margin: 18px 64px;
	float: left;
}
.linear{
	position:absolute; 
	width: 92px;
	border: 4px solid #ced1d6;
	float: left;
	margin-top:46px;
}
.linear_left{
	margin-left:-32px ;
}
.linear_right{
	margin-left:120px ;
}
.active{
	border-color:#5293c4 ;
}
.info{
	position: absolute;
	display: block;
	width: 120px;
	height: 18px;
	font-size: 12px;
	margin-left:36px;
	margin-top:80px ;
	text-align: center;
	line-height: 18px;
	color: #949ea7;
}
.font_active{
	color:#2b3d58;
}
.split{
	position: absolute;
	width: 700px;
	border:1px solid #eeeeee;
	left: 330px;
}
.input_span{
	display: block;
	width: 100px;
	height: 30px;
	float: left;
	color: #393939;
	font-size: 18px;
	margin-left:230px;
	text-align: center;
	line-height: 30px;
}
#input_div{
	display: block;
	float: left;
	width: 200px;
	height: 30px;
	text-align: center;
	line-height: 30px;
	border:1px solid #72aec2;
}
#input_div input{
	display: block;
	border:none;
	background: none;
	width: 190px;
	height: 26px;
	margin-left:4px;
	margin-top: 2px;
	font-size: 18px;
	outline: none;
}
.input_tip{
	display: block;
	width: 230px;
	height: 30px;
	float: left;
	line-height: 30px;
	color: red;
	font-size: 12px;
	font-weight: bold;
	padding-left: 10px;
}
.radio_span{
	display: block;
	width: 80px;
	height: 30px;
	float: left;
	line-height: 30px;
	font-size: 16px;
}
.radio_span input{
	display: block;
	float: left;
	margin-top: 8px;
}
.female{
	margin-left: 20px;
}
.next,.pre{
	display: block;
	width: 80px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	font-size: 16px;
	color: #fff;
	font-weight: bold;
	margin-top: 20px;
	float: left;
}
.pre{
	margin-left: 570px;
	background: #6fb3e0;
}
.next{
	margin-left: 30px;
	background:#87b87f;
}
.pre:hover,.next:hover{
	background: #ffb752;
}
#footer_box{
	width: 100%;
	height:100%;
	background: #f2f2f2;
	min-width: 1348px;
}
#footer{
	width: 1348px;
	height: 100px;
	text-align: center;
	margin: 0 auto;	
}
.split_bottom{
	width: 1288px;
	border: 1px solid #d5e3ef;
	margin-left: 30px;
}
#copy{
	display: block;
	width: 1000px;
	height: 30px;
	margin:8px auto ;
}
.select_list{
	display: block;
	width: 110px;
	height: 30px;
	float: left;
	font-size: 16px;
}
#submit_btn{
	border:none;
	background: none;
	display: block;
	width: 80px;
	height: 40px;
	background:#87b87f;
	line-height: 40px;
	text-align: center;
	font-size: 16px;
	color: #fff;
	font-weight: bold;
	margin-top: 20px;
	float: left;
	margin-left: 30px;	
}
#submit_btn:hover{
	background:#d6487e;
}
#success_message{
	display: block;
	width: 700px;
	height: 40px;
	background:url("../images/success.png") no-repeat 288px 16px #6fb3e0;
	text-align: center;
	line-height: 40px;
	font-weight: bold;
	color: #fff;
	margin-left: 50px;
	display: none;
}
#failed_message{
	display: block;
	width: 700px;
	height: 40px;
	background:url("../images/failed.png") no-repeat 290px 19px #d15b47;
	text-align: center;
	line-height: 40px;
	font-weight: bold;
	color: #fff;
	margin-left: 50px;
}
#back_to_regedit{
	display: block;
	width:200px ;
	height:60px;
	color: #fff;
	background: #8bbc67;
	text-align: center;
	font-weight: bold;
	line-height: 60px;
	margin:0 auto;
}
#back_to_login{
	display: block;
	width:200px ;
	height:60px;
	color: #fff;
	background:#efad62;
	text-align: center;
	font-weight: bold;
	line-height: 60px;
	margin:0 auto;
}
#back_to_regedit:hover,#back_to_login:hover{
	background:#d6487e;
}

#regedit_form{
	width: 100%;
	height: 100%;
}
#message{
	display: block;
	width:200px ;
	height: 48px;
	margin-left: 354px;
	line-height: 48px;
	color: red;
	font-weight: bold;
	font-size: 18px;
}
.tip{
	text-align: center;
	color: red;
	font-size: 12px;
}